{
  "version": 2,
  "buildCommand": "ng build --configuration=production",
  "outputDirectory": "dist/estudiemos-frontend",
  "env": {
    "ANGULAR_ENVIRONMENT": "production"
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\..*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
