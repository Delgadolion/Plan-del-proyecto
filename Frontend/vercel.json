{
  "version": 2,
  "builds": [
    {
      "src": "dist/frontend/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dist/frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/(?!.*\\.).*$",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/frontend/$1"
    }
  ]
}
